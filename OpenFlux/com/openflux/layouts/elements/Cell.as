package com.openflux.layout.elements{	import flash.display.DisplayObject;		public class Cell	{				protected var _link:DisplayObject;		protected var _x:Number;		protected var _y:Number;				public function Cell(x:Number=0, y:Number=0, link:DisplayObject=null)		{			this._link=link;			this._x=x;			this._y=y;		}				public function set link(value:DisplayObject):void		{			this._link=value;		}						public function get link():DisplayObject		{			return _link;		}				public function set x(value:Number):void		{			this._x=value;		}				public function get x():Number		{			return _x;		}				public function set y(value:Number):void		{			this._y=value;		}				public function get y():Number		{			return _y;		}	}}